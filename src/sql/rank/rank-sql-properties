gymRankList = select * from (select rownum as rnum, a.* from (select  gymreply.gymnumber,gym.gymbranchname, gym.gymdistrict, gym.gymaddress, gym.gymcategory, gym.gymhomepage, trunc(sum(gymreply.gymscore)/count(*),1) as score, gymimage.MAINIMAGE from gym join gymreply on(gym.gymnumber=gymreply.gymnumber) left join gymimage on(gym.gymnumber=gymimage.gymnumber) where gym.gymcategory = ? group by gymreply.gymnumber, gym.gymcategory, gym.gymdistrict, gymimage.mainimage, gym.gymbranchname,gym.gymaddress, gym.gymhomepage order by case when score>0 then score end desc, case when gymbranchname like '%%' then gymbranchname end asc)a) where rnum between 1 and 5
seoulgymRankList = select * from (select rownum as rnum, a.* from (select  gymreply.gymnumber,gym.gymbranchname, gym.gymdistrict, gym.gymaddress, gym.gymcategory, gym.gymhomepage, trunc(sum(gymreply.gymscore)/count(*),1) as score, gymimage.MAINIMAGE from gym join gymreply on(gym.gymnumber=gymreply.gymnumber) left join gymimage on(gym.gymnumber=gymimage.gymnumber) where gym.gymdistrict like '%서울%' and gym.gymcategory = ? group by gymreply.gymnumber, gym.gymcategory, gym.gymdistrict, gymimage.mainimage, gym.gymbranchname,gym.gymaddress, gym.gymhomepage order by case when score>0 then score end desc, case when gymbranchname like '%%' then gymbranchname end asc)a) where rnum between 1 and 5
gyeongigymRankList = select * from (select rownum as rnum, a.* from (select  gymreply.gymnumber,gym.gymbranchname, gym.gymdistrict, gym.gymaddress, gym.gymcategory, gym.gymhomepage, trunc(sum(gymreply.gymscore)/count(*),1) as score, gymimage.MAINIMAGE from gym join gymreply on(gym.gymnumber=gymreply.gymnumber) left join gymimage on(gym.gymnumber=gymimage.gymnumber) where gym.gymdistrict like '%경기%' and gym.gymcategory = ? group by gymreply.gymnumber, gym.gymcategory, gym.gymdistrict, gymimage.mainimage, gym.gymbranchname,gym.gymaddress, gym.gymhomepage order by case when score>0 then score end desc, case when gymbranchname like '%%' then gymbranchname end asc)a) where rnum between 1 and 5